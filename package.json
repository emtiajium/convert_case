{
  "name": "convert_case",
  "version": "0.0.1",
  "description": "A desktop app mimicking https://convertcase.net/",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . \"Convert Case\" --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icon/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . \"Convert Case\" --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://bitbucket.org/emtiajium/convert_case",
  "author": "271emtiaj@gmail.com",
  "devDependencies": {
    "electron": "^3.0.0",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "prepositions": "0.0.0",
    "underscore": "^1.9.1",
    "underscore.string": "^3.3.5"
  }
}
